                "id": "c51ef49e-627b-4fe5-a6fb-cb93d7914dba",
                "type": "text/javascript",
                "exec": [
                  "postman.setGlobalVariable(\"loadUtils\", function loadUtils() {",
                  "    let utils = {};",
                  "",
                  "    utils.randomUUID = function randomUUID() {",
                  "        var randomId = Math.abs(Math.floor(Math.random() * Number.MIN_SAFE_INTEGER));",
                  "        return randomId;",
                  "    }",
                  "",
                  "    return utils;",
                  "",
                  "} + '; loadUtils();');",
                  "tests['Utils initialized'] = true;"
                ]
              }
            }
          ],
